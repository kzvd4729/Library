/*
pi[i] is the length of the longest proper
prefix of the substring s[0â€¦i] which is
also a suffix of this substring.
*/
vector<int>prefixFunction(string s)
{
  int n=s.size();vector<int>pi(n);
  for(int i=1;i<n;i++)
  {
    int j=pi[i-1];
    while(j>0&&s[i]!=s[j])j=pi[j-1];
    if(s[i]==s[j])j++;pi[i]=j;
  }
  return pi;
}
