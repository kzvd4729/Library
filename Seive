/************ Seive **************/
int Is_Prime[N+2];vector<int>Prime;
void Seive(int n)
{
  Prime.push_back(2);Is_Prime[1]=1;
  for(int i=2;i<=n;i+=2)Is_Prime[i]=1;
  int sq=sqrt(n+1);
  for(int i=3;i<=n;i+=2)
  {
    if(Is_Prime[i])continue;Prime.push_back(i);
    if(i>sq)continue;
    for(int j=i*i;j<=n;j+=2*i)
      Is_Prime[j]=1;
  }
}
