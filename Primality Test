/******************* Fermet Test ****************/
long Bigmod(__int128 b,long p,__int128 mod)
{
  __int128 ret=1;
  while(p)
  {
    if(p%2)ret=(ret*b)%mod;
    b=(b*b)%mod;p/=2;
  }
  return ret;
}
bool Fermet(long n,int k)
{
  if(n<=1||n==4)return false;
  if(n<=3)return true;
  while(k--)
  {
    long a=2+rand()%(n-4);
    if(Bigmod(a,n-1,n)!=1)
      return false;
  }
  return true;
}
